{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "amazon_s3",
		"amazon_s3": {
			"bucket": "",
			"credentials": {
				"id": "",
				"role": "",
				"secret": "",
				"token": ""
			},
			"delete_objects": false,
			"prefix": "",
			"region": "eu-west-1",
			"sqs_body_path": "Records.s3.object.key",
			"sqs_envelope_path": "",
			"sqs_max_messages": 10,
			"sqs_url": "",
			"timeout_s": 5
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "amazon_s3",
		"amazon_s3": {
			"bucket": "",
			"credentials": {
				"id": "",
				"role": "",
				"secret": "",
				"token": ""
			},
			"path": "${!count:files}-${!timestamp_unix_nano}.txt",
			"region": "eu-west-1",
			"timeout_s": 5
		}
	},
	"resources": {
		"caches": {},
		"conditions": {}
	},
	"logger": {
		"prefix": "benthos",
		"log_level": "INFO",
		"add_timestamp": true,
		"json_format": true
	},
	"metrics": {
		"type": "http_server",
		"prefix": "benthos",
		"http_server": {},
		"prometheus": {},
		"statsd": {
			"address": "localhost:4040",
			"flush_period": "100ms",
			"max_packet_size": 1440,
			"network": "udp"
		}
	}
}