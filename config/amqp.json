{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "amqp",
		"amqp": {
			"bindings_declare": [],
			"consumer_tag": "benthos-consumer",
			"prefetch_count": 10,
			"prefetch_size": 0,
			"queue": "benthos-queue",
			"queue_declare": {
				"durable": true,
				"enabled": false
			},
			"tls": {
				"client_certs": [],
				"enabled": false,
				"root_cas_file": "",
				"skip_cert_verify": false
			},
			"url": "amqp://guest:guest@localhost:5672/"
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "amqp",
		"amqp": {
			"exchange": "benthos-exchange",
			"exchange_declare": {
				"durable": true,
				"enabled": false,
				"type": "direct"
			},
			"immediate": false,
			"key": "benthos-key",
			"mandatory": false,
			"persistent": false,
			"tls": {
				"client_certs": [],
				"enabled": false,
				"root_cas_file": "",
				"skip_cert_verify": false
			},
			"url": "amqp://guest:guest@localhost:5672/"
		}
	},
	"resources": {
		"caches": {},
		"conditions": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true
	},
	"metrics": {
		"type": "http_server",
		"prefix": "benthos",
		"http_server": {},
		"prometheus": {},
		"statsd": {
			"address": "localhost:4040",
			"flush_period": "100ms",
			"network": "udp"
		}
	}
}