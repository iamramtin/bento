{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "redis_streams",
		"redis_streams": {
			"body_key": "body",
			"client_id": "benthos_consumer",
			"commit_period_ms": 1000,
			"consumer_group": "benthos_group",
			"limit": 10,
			"start_from_oldest": true,
			"streams": [
				"benthos_stream"
			],
			"timeout_ms": 5000,
			"url": "tcp://localhost:6379"
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "redis_streams",
		"redis_streams": {
			"body_key": "body",
			"max_length": 0,
			"stream": "benthos_stream",
			"url": "tcp://localhost:6379"
		}
	},
	"resources": {
		"caches": {},
		"conditions": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true
	},
	"metrics": {
		"type": "http_server",
		"prefix": "benthos",
		"http_server": {},
		"prometheus": {},
		"statsd": {
			"address": "localhost:4040",
			"flush_period": "100ms",
			"network": "udp"
		}
	}
}