{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout": "5s",
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "redis_streams",
		"redis_streams": {
			"body_key": "body",
			"client_id": "benthos_consumer",
			"commit_period": "1s",
			"consumer_group": "benthos_group",
			"limit": 10,
			"start_from_oldest": true,
			"streams": [
				"benthos_stream"
			],
			"timeout": "5s",
			"url": "tcp://localhost:6379"
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "redis_streams",
		"redis_streams": {
			"body_key": "body",
			"max_length": 0,
			"stream": "benthos_stream",
			"url": "tcp://localhost:6379"
		}
	},
	"resources": {
		"caches": {},
		"conditions": {},
		"rate_limits": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true,
		"static_fields": {
			"@service": "benthos"
		}
	},
	"metrics": {
		"type": "http_server",
		"http_server": {},
		"prefix": "benthos"
	},
	"tracer": {
		"type": "none",
		"none": {}
	},
	"shutdown_timeout": "20s"
}