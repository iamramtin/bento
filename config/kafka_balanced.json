{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout": "5s",
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "kafka_balanced",
		"kafka_balanced": {
			"addresses": [
				"localhost:9092"
			],
			"client_id": "benthos_kafka_input",
			"commit_period": "1s",
			"consumer_group": "benthos_consumer_group",
			"max_batch_count": 1,
			"start_from_oldest": true,
			"target_version": "1.0.0",
			"tls": {
				"client_certs": [],
				"enabled": false,
				"root_cas_file": "",
				"skip_cert_verify": false
			},
			"topics": [
				"benthos_stream"
			]
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "stdout",
		"stdout": {
			"delimiter": ""
		}
	},
	"resources": {
		"caches": {},
		"conditions": {},
		"rate_limits": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true,
		"static_fields": {
			"@service": "benthos"
		}
	},
	"metrics": {
		"type": "http_server",
		"http_server": {},
		"prefix": "benthos"
	},
	"tracer": {
		"type": "none",
		"none": {}
	},
	"shutdown_timeout": "20s"
}