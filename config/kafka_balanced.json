{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"debug_endpoints": false
	},
	"input": {
		"kafka_balanced": {
			"addresses": [
				"localhost:9092"
			],
			"client_id": "benthos_kafka_input",
			"consumer_group": "benthos_consumer_group",
			"start_from_oldest": true,
			"topics": [
				"benthos_stream"
			]
		},
		"type": "kafka_balanced"
	},
	"buffer": {
		"none": {},
		"type": "none"
	},
	"pipeline": {
		"processors": [
			{
				"bounds_check": {
					"max_part_size": 1073741824,
					"max_parts": 100,
					"min_part_size": 1,
					"min_parts": 1
				},
				"type": "bounds_check"
			}
		],
		"threads": 1
	},
	"output": {
		"stdout": {
			"delimiter": ""
		},
		"type": "stdout"
	}
}