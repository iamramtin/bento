{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "stdin",
		"stdin": {
			"delimiter": "",
			"max_buffer": 1000000,
			"multipart": false
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [
			{
				"type": "http",
				"http": {
					"max_parallel": 0,
					"parallel": false,
					"request": {
						"backoff_on": [
							429
						],
						"basic_auth": {
							"enabled": false,
							"password": "",
							"username": ""
						},
						"drop_on": [],
						"headers": {
							"Content-Type": "application/octet-stream"
						},
						"max_retry_backoff_ms": 300000,
						"oauth": {
							"access_token": "",
							"access_token_secret": "",
							"consumer_key": "",
							"consumer_secret": "",
							"enabled": false,
							"request_url": ""
						},
						"retries": 3,
						"retry_period_ms": 1000,
						"timeout_ms": 5000,
						"tls": {
							"client_certs": [],
							"enabled": false,
							"root_cas_file": "",
							"skip_cert_verify": false
						},
						"url": "http://localhost:4195/post",
						"verb": "POST"
					}
				}
			}
		],
		"threads": 1
	},
	"output": {
		"type": "stdout",
		"stdout": {
			"delimiter": ""
		}
	},
	"resources": {
		"caches": {},
		"conditions": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true
	},
	"metrics": {
		"type": "http_server",
		"prefix": "benthos",
		"http_server": {},
		"prometheus": {},
		"statsd": {
			"address": "localhost:4040",
			"flush_period": "100ms",
			"network": "udp"
		}
	}
}