{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "stdin",
		"stdin": {
			"delimiter": "",
			"max_buffer": 1000000,
			"multipart": false
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [
			{
				"type": "bounds_check",
				"bounds_check": {
					"max_part_size": 1073741824,
					"max_parts": 100,
					"min_part_size": 1,
					"min_parts": 1
				}
			}
		],
		"threads": 1
	},
	"output": {
		"type": "stdout",
		"stdout": {
			"delimiter": ""
		}
	},
	"resources": {
		"caches": {},
		"conditions": {}
	},
	"logger": {
		"prefix": "benthos",
		"log_level": "INFO",
		"add_timestamp": true,
		"json_format": true
	},
	"metrics": {
		"type": "http_server",
		"prefix": "benthos",
		"http_server": {},
		"prometheus": {},
		"statsd": {
			"address": "localhost:4040",
			"flush_period": "100ms",
			"max_packet_size": 1440,
			"network": "udp"
		}
	}
}