{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout": "5s",
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "stdin",
		"stdin": {
			"delimiter": "",
			"max_buffer": 1000000,
			"multipart": false
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [
			{
				"type": "awk",
				"awk": {
					"codec": "text",
					"parts": [],
					"program": "BEGIN { x = 0 } { print $0, x; x++ }"
				}
			}
		],
		"threads": 1
	},
	"output": {
		"type": "stdout",
		"stdout": {
			"delimiter": ""
		}
	},
	"resources": {
		"caches": {},
		"conditions": {},
		"rate_limits": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true,
		"static_fields": {
			"@service": "benthos"
		}
	},
	"metrics": {
		"type": "http_server",
		"http_server": {},
		"prefix": "benthos"
	},
	"tracer": {
		"type": "none",
		"none": {}
	},
	"shutdown_timeout": "20s"
}