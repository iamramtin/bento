{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout": "5s",
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "nanomsg",
		"nanomsg": {
			"bind": true,
			"poll_timeout": "5s",
			"reply_timeout": "5s",
			"socket_type": "PULL",
			"sub_filters": [],
			"urls": [
				"tcp://*:5555"
			]
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "nanomsg",
		"nanomsg": {
			"bind": false,
			"poll_timeout": "5s",
			"socket_type": "PUSH",
			"urls": [
				"tcp://localhost:5556"
			]
		}
	},
	"resources": {
		"caches": {},
		"conditions": {},
		"rate_limits": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true,
		"static_fields": {
			"@service": "benthos"
		}
	},
	"metrics": {
		"type": "http_server",
		"http_server": {},
		"prefix": "benthos"
	},
	"tracer": {
		"type": "none",
		"none": {}
	},
	"shutdown_timeout": "20s"
}