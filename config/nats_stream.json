{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "nats_stream",
		"nats_stream": {
			"client_id": "benthos_client",
			"cluster_id": "test-cluster",
			"durable_name": "benthos_offset",
			"queue": "benthos_queue",
			"start_from_oldest": true,
			"subject": "benthos_messages",
			"urls": [
				"nats://localhost:4222"
			]
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "nats_stream",
		"nats_stream": {
			"client_id": "benthos_client",
			"cluster_id": "test-cluster",
			"subject": "benthos_messages",
			"urls": [
				"nats://localhost:4222"
			]
		}
	},
	"resources": {
		"caches": {},
		"conditions": {}
	},
	"logger": {
		"prefix": "benthos",
		"log_level": "INFO",
		"add_timestamp": true,
		"json_format": true
	},
	"metrics": {
		"type": "http_server",
		"prefix": "benthos",
		"http_server": {},
		"prometheus": {},
		"statsd": {
			"address": "localhost:4040",
			"flush_period": "100ms",
			"max_packet_size": 1440,
			"network": "udp"
		}
	}
}