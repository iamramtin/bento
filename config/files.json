{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout": "5s",
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "files",
		"files": {
			"path": ""
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "files",
		"files": {
			"path": "${!count:files}-${!timestamp_unix_nano}.txt"
		}
	},
	"resources": {
		"caches": {},
		"conditions": {},
		"rate_limits": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true,
		"static_fields": {
			"@service": "benthos"
		}
	},
	"metrics": {
		"type": "http_server",
		"http_server": {},
		"prefix": "benthos"
	},
	"tracer": {
		"type": "none",
		"none": {}
	},
	"shutdown_timeout": "20s"
}