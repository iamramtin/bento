{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout_ms": 5000,
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "stdin",
		"stdin": {
			"delimiter": "",
			"max_buffer": 1000000,
			"multipart": false
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "retry",
		"retry": {
			"backoff": {
				"initial_interval": "500ms",
				"max_elapsed_time": "0s",
				"max_interval": "3s"
			},
			"max_retries": 0,
			"output": {}
		}
	},
	"resources": {
		"caches": {},
		"conditions": {},
		"rate_limits": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true,
		"static_fields": {
			"@service": "benthos"
		}
	},
	"metrics": {
		"type": "http_server",
		"http_server": {},
		"prefix": "benthos"
	},
	"sys_exit_timeout_ms": 20000
