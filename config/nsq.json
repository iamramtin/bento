{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout": "5s",
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "nsq",
		"nsq": {
			"channel": "benthos_stream",
			"lookupd_http_addresses": [
				"localhost:4161"
			],
			"max_in_flight": 100,
			"nsqd_tcp_addresses": [
				"localhost:4150"
			],
			"topic": "benthos_messages",
			"user_agent": "benthos_consumer"
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "nsq",
		"nsq": {
			"nsqd_tcp_address": "localhost:4150",
			"topic": "benthos_messages",
			"user_agent": "benthos_producer"
		}
	},
	"resources": {
		"caches": {},
		"conditions": {},
		"rate_limits": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true,
		"static_fields": {
			"@service": "benthos"
		}
	},
	"metrics": {
		"type": "http_server",
		"http_server": {},
		"prefix": "benthos"
	},
	"tracer": {
		"type": "none",
		"none": {}
	},
	"shutdown_timeout": "20s"
}