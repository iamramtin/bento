{
	"http": {
		"address": "0.0.0.0:4195",
		"read_timeout": "5s",
		"root_path": "/benthos",
		"debug_endpoints": false
	},
	"input": {
		"type": "mqtt",
		"mqtt": {
			"client_id": "benthos_input",
			"qos": 1,
			"topics": [
				"benthos_topic"
			],
			"urls": [
				"tcp://localhost:1883"
			]
		}
	},
	"buffer": {
		"type": "none",
		"none": {}
	},
	"pipeline": {
		"processors": [],
		"threads": 1
	},
	"output": {
		"type": "mqtt",
		"mqtt": {
			"client_id": "benthos_output",
			"qos": 1,
			"topic": "benthos_topic",
			"urls": [
				"tcp://localhost:1883"
			]
		}
	},
	"resources": {
		"caches": {},
		"conditions": {},
		"rate_limits": {}
	},
	"logger": {
		"prefix": "benthos",
		"level": "INFO",
		"add_timestamp": true,
		"json_format": true,
		"static_fields": {
			"@service": "benthos"
		}
	},
	"metrics": {
		"type": "http_server",
		"http_server": {},
		"prefix": "benthos"
	},
	"tracer": {
		"type": "none",
		"none": {}
	},
	"shutdown_timeout": "20s"
}